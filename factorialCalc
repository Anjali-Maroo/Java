import java.util.Scanner;
class Main{
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a number: ");
        int n = sc.nextInt();
        long fact =1;
        while(n!=0){
            fact = fact * n;
            n--;
        }
        System.out.print("The factorial is "+ fact);
        sc.close();
    }
}
